;; shadow-cljs configuration
{:source-paths ["src"]

 :dependencies
 [[com.kiranshila/cybermonday "0.2.1"]
  [reagent/reagent "1.1.0"]]

 :builds
 {:app
  {:target :browser
   :output-dir "public/js"
   :asset-path "/js"
   :compiler-options {:output-feature-set :es8}
   :modules {:main
             {:init-fn main.core/init}}
   :devtools {:http-root "public"
              :http-port 8020}}}}
