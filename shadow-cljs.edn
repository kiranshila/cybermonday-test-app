;; shadow-cljs configuration
{:source-paths ["src"]

 :dependencies
 [[com.kiranshila/cybermonday "0.0.17"]
  [reagent/reagent "1.1.0"]]

 :dev-http {8020 "public"}

 :builds
 {:app
  {:target :browser
   :output-dir "public/js"
   :asset-path "/js"
   :compiler-options {:output-feature-set :es8}
   :modules {:main
             {:init-fn main.core/init}}}}}
